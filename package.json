{
  "name": "aremycolorsaccessible",
  "version": "1.3.0",
  "engines": {
    "node": "9.5.x"
  },
  "scripts": {
    "now-build": "next build",
    "build": "next build",
    "dev": "next",
    "analyze_bundle": "BUNDLE_ANALYZE=both npm run build",
    "analyze_bundle:browser": "BUNDLE_ANALYZE=browser npm run build",
    "analyze_bundle:server": "BUNDLE_ANALYZE=server npm run build",
    "precommit": "lint-staged",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --max-warnings 0 --format codeframe",
    "test": "npm run lint && NODE_ENV=test jest",
    "cypress:open": "npm run dev & cypress open",
    "cypress:run": "cypress run",
    "tsc": "tsc",
    "gen": "plop --plopfile generators/plopfile.js",
    "gen:common": "plop --plopfile generators/plopfile.js common",
    "gen:component": "plop --plopfile generators/plopfile.js component",
    "gen:page": "plop --plopfile generators/plopfile.js page"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Simon Taggart <me@simontaggart.com> (http://simontaggart.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiTaggart/AreMyColorsAccessible"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.1.6",
    "@types/react-test-renderer": "16.0.3",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "ajv": "6.7.0",
    "autoprefixer": "9.4.3",
    "babel-core": "7.0.0-bridge.0",
    "breakpoint-sass": "2.7.1",
    "classnames": "2.2.6",
    "color": "3.1.0",
    "lodash": "4.17.11",
    "next": "8.0.0-canary.19",
    "next-compose-plugins": "2.1.1",
    "node-sass": "4.11.0",
    "normalize.css": "8.0.1",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "overrides": [
      {
        "files": "**/*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/color": "3.0.0",
    "@types/compression": "0.0.36",
    "@types/enzyme": "3.1.15",
    "@types/express": "4.16.1",
    "@types/jest": "23.3.10",
    "@types/next": "7.0.5",
    "@types/node": "10.12.18",
    "@types/query-string": "6.1.1",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@typescript-eslint/parser": "1.3.0",
    "acorn": "6.1.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "cypress": "3.1.3",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.9.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-cypress": "2.1.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "listr": "0.14.3",
    "plop": "2.2.0",
    "prettier": "1.15.3",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.7.0",
    "regenerator-runtime": "0.13.1",
    "typescript": "3.3.3"
  }
}
