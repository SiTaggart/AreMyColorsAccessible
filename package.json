{
  "name": "aremycolorsaccessible",
  "version": "1.3.0",
  "engines": {
    "node": "16.17.1"
  },
  "scripts": {
    "clean": "rm -rf .next/",
    "build": "next build",
    "dev": "next",
    "bundlesize": "bundlesize",
    "jest": "NODE_ENV=test jest",
    "jest:coverage": "yarn jest --coverage",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --format codeframe",
    "test": "yarn lint && NODE_ENV=test jest",
    "cypress": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress:open'",
    "cypress:open": "cypress open",
    "cypress:run": "yarn percy:exec cypress run",
    "cypress:run-and-serve": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress:run'",
    "percy": "percy",
    "percy:exec": "yarn percy exec --",
    "prettier": "prettier --list-different './**/*.{js,jsx,ts,tsx}'",
    "prettier-clean": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "tsc": "tsc",
    "ci:checks": "yarn clean && yarn build && yarn lint && yarn jest && yarn bundlewatch && yarn cypress:run-and-serve",
    "prepare": "husky install"
  },
  "author": "Simon Taggart <me@simontaggart.com> (http://simontaggart.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiTaggart/AreMyColorsAccessible"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.19.1",
    "@emotion/css": "11.10.0",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@twilio-paste/core": "15.3.0",
    "@twilio-paste/icons": "9.2.0",
    "color": "4.2.3",
    "color-combos": "1.0.17",
    "cors": "2.8.5",
    "cypress": "10.8.0",
    "lodash": "4.17.21",
    "next": "12.3.1",
    "normalize.css": "8.0.1",
    "polished": "4.2.2",
    "prism-react-renderer": "1.3.5",
    "prop-types": "15.8.1",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.2",
    "@emotion/eslint-plugin": "11.10.0",
    "@emotion/jest": "11.10.0",
    "@jest/globals": "29.0.3",
    "@netlify/plugin-lighthouse": "3.4.1",
    "@netlify/plugin-nextjs": "4.23.1",
    "@next/eslint-plugin-next": "12.3.1",
    "@percy/cli": "1.10.4",
    "@percy/cypress": "3.1.2",
    "@sitaggart/eslint-config-ts": "0.6.20",
    "@sitaggart/prettier-config": "0.3.19",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/color": "3.0.3",
    "@types/cors": "2.8.12",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "17.0.45",
    "@types/query-string": "6.3.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.6",
    "babel-jest": "29.0.3",
    "bundlewatch": "0.3.3",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-formatter-codeframe": "7.32.1",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "netlify-plugin-cache-nextjs": "1.6.1",
    "pinst": "3.0.0",
    "precise-commits": "1.0.2",
    "prettier": "2.7.1",
    "react-addons-test-utils": "15.6.2",
    "start-server-and-test": "1.14.0",
    "typescript": "4.8.3"
  },
  "resolutions": {
    "@types/react": "17.0.50"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./.next/static/**/pages/*.js",
        "maxSize": "70 kB"
      },
      {
        "path": "./.next/static/chunks/*.js",
        "maxSize": "100 kB"
      },
      {
        "path": "./.next/server/pages/*.js",
        "maxSize": "580 kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ],
      "repoBranchBase": "main"
    }
  },
  "packageManager": "yarn@3.2.3"
}
