{
  "env": {
    "development": {
      "presets": [
        [
          "next/babel",
          {
            "preset-react": {
              "runtime": "automatic",
              "importSource": "@emotion/react"
            }
          }
        ]
      ],
      "plugins": [["@emotion", { "labelFormat": "[dirname]-[local]" }]]
    },
    "production": {
      "presets": [
        [
          "next/babel",
          {
            "preset-react": {
              "runtime": "automatic",
              "importSource": "@emotion/react"
            }
          }
        ]
      ],
      "plugins": ["@emotion"]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-react": {
              "runtime": "automatic",
              "importSource": "@emotion/react"
            },
            "preset-env": { "modules": "commonjs" }
          }
        ]
      ],
      "plugins": ["@emotion"]
    }
  }
}
