{
  "env": {
    "development": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": [["emotion", { "labelFormat": "[dirname]-[local]" }]]
    },
    "production": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": ["emotion"]
    },
    "test": {
      "presets": [
        ["next/babel", { "preset-env": { "modules": "commonjs" } }],
        "@zeit/next-typescript/babel"
      ],
      "plugins": ["emotion"]
    }
  }
}
