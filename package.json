{
  "name": "aremycolorsaccessible",
  "version": "1.3.0",
  "engines": {
    "node": "9.5.x"
  },
  "scripts": {
    "old_dev": "next",
    "old_start": "NODE_ENV=production next start -p $PORT",
    "build": "next build",
    "dev": "node server.ts",
    "start": "NODE_ENV=production node server.ts -p $PORT",
    "analyze_bundle": "BUNDLE_ANALYZE=both npm run build",
    "analyze_bundle:browser": "BUNDLE_ANALYZE=browser npm run build",
    "analyze_bundle:server": "BUNDLE_ANALYZE=server npm run build",
    "precommit": "lint-staged",
    "heroku-postbuild": "next build",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --max-warnings 0 --format codeframe",
    "test": "npm run lint && NODE_ENV=test jest",
    "cypress:open": "npm run dev & cypress open",
    "cypress:run": "cypress run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Simon Taggart <me@simontaggart.com> (http://simontaggart.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiTaggart/AreMyColorsAccessible"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-sass": "1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autoprefixer": "9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "breakpoint-sass": "2.7.1",
    "colorable": "1.0.5",
    "compression": "1.7.3",
    "express": "4.16.3",
    "lodash": "4.17.11",
    "next": "7.0.1",
    "next-compose-plugins": "2.1.1",
    "node-sass": "4.9.3",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/next": "^7.0.1",
    "@types/node": "^10.11.4",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cypress": "^3.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
