{
  "name": "aremycolorsaccessible",
  "version": "1.3.0",
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "clean": "rm -rf .next/",
    "build": "next build",
    "dev": "next",
    "bundlesize": "bundlesize",
    "jest": "NODE_ENV=test jest",
    "jest:coverage": "yarn jest --coverage",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --format codeframe",
    "test": "yarn lint && NODE_ENV=test jest",
    "cypress": "start-server-and-test dev http://localhost:3000 cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "yarn percy:exec cypress run",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cypress:run",
    "percy": "percy",
    "percy:exec": "percy exec --",
    "prettier": "prettier --list-different './**/*.{js,jsx,ts,tsx}'",
    "prettier-clean": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "tsc": "tsc",
    "ci:checks": "yarn clean && yarn build && yarn lint && yarn tsc && yarn jest && yarn bundlewatch && yarn cypress:ci",
    "prepare": "husky install"
  },
  "author": "Simon Taggart <me@simontaggart.com> (http://simontaggart.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiTaggart/AreMyColorsAccessible"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.17.10",
    "@emotion/css": "11.9.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@twilio-paste/core": "11.5.0",
    "@twilio-paste/icons": "8.0.2",
    "color": "4.2.3",
    "color-combos": "1.0.17",
    "cors": "2.8.5",
    "lodash": "4.17.21",
    "next": "12.1.6",
    "normalize.css": "8.0.1",
    "polished": "4.2.2",
    "prism-react-renderer": "1.3.1",
    "prop-types": "15.8.1",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.9.2",
    "@emotion/eslint-plugin": "11.7.0",
    "@emotion/jest": "11.9.1",
    "@netlify/plugin-lighthouse": "2.1.3",
    "@netlify/plugin-nextjs": "4.7.0",
    "@next/eslint-plugin-next": "12.1.6",
    "@percy/cli": "1.1.2",
    "@percy/cypress": "3.1.1",
    "@sitaggart/eslint-config-ts": "0.6.10",
    "@sitaggart/prettier-config": "0.3.9",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.0",
    "@types/color": "3.0.3",
    "@types/cors": "2.8.12",
    "@types/jest": "27.5.0",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.8",
    "@types/query-string": "6.3.0",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.16",
    "@types/react-test-renderer": "17.0.2",
    "babel-jest": "27.5.1",
    "bundlewatch": "0.3.3",
    "cypress": "9.6.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-formatter-codeframe": "7.32.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "netlify-plugin-cache-nextjs": "1.6.1",
    "pinst": "3.0.0",
    "precise-commits": "1.0.2",
    "prettier": "2.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "17.0.2",
    "start-server-and-test": "1.14.0",
    "typescript": "4.6.4"
  },
  "resolutions": {
    "@types/react": "17.0.45"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./.next/static/**/pages/*.js",
        "maxSize": "70 kB"
      },
      {
        "path": "./.next/static/chunks/*.js",
        "maxSize": "100 kB"
      },
      {
        "path": "./.next/server/pages/*.js",
        "maxSize": "580 kB"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ],
      "repoBranchBase": "main"
    }
  }
}
