// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Installation {
  id              String   @id
  team_name       String
  enterprise_name String?
  enterprise_id   Int?
  install_date    DateTime @default(now())
}
