{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [["emotion", { "labelFormat": "[dirname]-[local]" }]]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": ["emotion"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]],
      "plugins": ["emotion"]
    }
  }
}
